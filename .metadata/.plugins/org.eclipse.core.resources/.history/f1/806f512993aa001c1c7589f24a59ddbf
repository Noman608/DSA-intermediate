
public class Queue {
	private class Node {
		int data;
		Node next;
	}

	private Node head = null;
	private Node tail = null;
	private int size;
	
	public void display() {
		Node temp =  head;
		System.out.println("----------");
		 while(temp!=null) {
			temp = temp.next;
		}
		System.out.println("----------");
	}
}
