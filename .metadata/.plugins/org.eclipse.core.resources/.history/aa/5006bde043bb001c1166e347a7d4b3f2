
public class Practice {
	public static void main(String[] args) {
		int[] array = { 10, 5, 20, 30, 25 }; 
	      System.out.println(liniarSearch(array, 35));
	}
    public static boolean liniarSearch(int arr[],int item) {
		for(int i=0 ;i<arr.length;i++) {
			if(arr[i]==item) {
				return true;
			}
		}
		return false;
	}

}
